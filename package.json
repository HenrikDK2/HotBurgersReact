{
  "name": "HotBurgersReact",
  "version": "1.0.0",
  "description": "HotBurgers er en fast-food hjemmeside, frems til din egen burger.",
  "main": "index.js",
  "scripts": {
    "dev": "parcel src/index.html",
    "build": "(npm run del) & (parcel build src/index.html --public-url ./) & (gulp build)",
    "del": "del-cli --force ./dist .cache .parcel-cache",
    "reset": "(del-cli --force ./dist .cache .parcel-cache ./node_modules) & npm i",
    "resetDev": "(del-cli --force ./dist .cache .parcel-cache ./node_modules) & (npm i) & npm run dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HenrikDK2/HotBurgersReact.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/HenrikDK2/HotBurgersReact/issues"
  },
  "homepage": "https://github.com/HenrikDK2/HotBurgersReact#readme",
  "dependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "react-router-dom": "^5.1.2",
    "gulp-imagemin": "^7.1.0",
    "gulp-uglify": "^3.0.2",
    "gulp": "^4.0.2",
    "gulp-htmlmin": "^5.0.1",
    "@emotion/core": "^10.0.28",
    "del-cli": "^3.0.0",
    "emotion": "^10.0.27",
    "emotion-theming": "^10.0.27",
    "parcel-bundler": "^1.12.4"
  }
}
